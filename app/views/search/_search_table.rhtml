<table id="searchTable">
  <thead>
    <tr>
        <th>Recipe Title</th>
        <th>Prep Time</th>
    </tr>
  </thead>
  <tbody>
    <% for result in @results %>
    <tr>    
        <td><%= link_to result.title, recipe_path(result.id) %></td>
        <td><%= result.prep_time%></td>
    </tr>
    <%end%>
  </tbody>
</table>

